<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  <log4net source="Config/log4net.config" />

  <appSettings>
    <add key="finishWord" value=":quit"/>
    <add key="punctuationMarks" value=",.;"/>
    <add key="textExtension" value="*.txt"/>
    <add key="numberOfResultsToShow" value="10"/>
  </appSettings>

  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>